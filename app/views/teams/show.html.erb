<div class="col-md-12">
  <h2><%= @team.name %></h2>
  <label class="label-info">所有项目</label>
  <div class="pull-right">
    <%= link_to("创建项目", new_team_project_path(@team), class: "btn btn-primary") %>
  </div>
  <br>
  <br>
  <hr>
  <div class="row">
    <% @projects.each do |project| %>
      <div class="col-md-3 col-sm-6 text-center">
        <%= link_to(project.title, project_path(project)) %>
      </div>
    <% end %>
  </div>
</div>
